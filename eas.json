{
  "cli": {
    "version": ">= 7.6.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
        "distribution": "internal",
        "image": "latest"
      },
      "ios": {
        "simulator": true,
        "autoIncrement": "buildNumber"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "image": "latest",
        "autoIncrement": "versionCode"
      },
      "ios": {
        "image": "latest",
        "autoIncrement": "buildNumber"
      }
    }
  }
}
